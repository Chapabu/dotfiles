#! /usr/bin/env bash
echo "Loaded applications..."

# Drupal console is now loaded when required with the load_drupal_console function.

# NVM
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"

#GPG
export GPG_TTY=$(tty)

eval $(thefuck --alias)

export DENO_INSTALL="/home/mattc/.deno"
